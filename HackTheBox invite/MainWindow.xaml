        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HTB_code_generator"
        mc:Ignorable="d"
        Title="HTB invite creator" Height="344.789" Width="328.451">
    <Grid Background="#FF232323">
        <ListBox x:Name="log" HorizontalAlignment="Left" Height="269" Margin="6,10,0,0" VerticalAlignment="Top" Width="300" Background="#FF373737" BorderBrush="{x:Null}" RenderTransformOrigin="0.285,0.478" Foreground="White"/>
        <Button x:Name="Generate" Content="Generate invite code
" HorizontalAlignment="Left" Margin="6,281,0,0" VerticalAlignment="Top" Width="144" BorderBrush="{x:Null}" Background="#FF373737" Foreground="White" Click="Generate_Click"/>
        <Button x:Name="copyCode" Content="Copy code" HorizontalAlignment="Left" Margin="155,281,0,0" VerticalAlignment="Top" Width="151" BorderBrush="{x:Null}" Background="#FF373737" Foreground="White" Click="copyCode_Click"/>

    </Grid>
</Window>
